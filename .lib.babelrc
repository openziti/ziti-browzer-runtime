{
  "presets": [
    ["@babel/env", {
      "useBuiltIns": "usage",
      "corejs": 3,
      "targets": {
        "browsers": [ "> 1%", "last 2 versions", "ie 9" ]
      }
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-regenerator", {
      "asyncGenerators": true,
      "generators": true,
      "async": true
    }]
  ]
}
