{
  "name": "@openziti/ziti-browzer-runtime",
  "version": "0.1.0",
  "description": "A JavaScript runtime that is auto-injected into the Page of a Zitified web app",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha -t 30000 -R ${REPORTER:-spec} tests/unit/index.js",
    "test-karma": "karma start",
    "coverage": "rm -rf coverage && nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage-report": "nyc report",
    "browserify": "browserify src/node/ziti-browzer-runtime.js -o dist/ziti-browzer-runtime.js -s ziti",
    "build": "npm run build:clean && npm run build:lib && npm run build:dist",
    "build:clean": "rimraf lib dist",
    "build:lib": "babel --config-file ./.lib.babelrc src --out-dir lib",
    "build:dist": "npm run browserify && npm run minify",
    "minify": "terser --compress --mangle -o dist/ziti-browzer-runtime.min.js dist/ziti-browzer-runtime.js"
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openziti/ziti-browzer-runtime.git"
  },
  "keywords": [
    "ziti",
    "js",
    "javascript",
    "zero trust",
    "zero-trust",
    "browZer",
    "browser"
  ],
  "author": "NetFoundry",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/openziti/ziti-browzer-runtime/issues"
  },
  "homepage": "https://github.com/openziti/ziti-browzer-runtime#readme",
  "lint-staged": {
    "linters": {
      "*.js": [
        "xo --fix",
        "git add"
      ],
      "*.md": [
        "remark . -qfo",
        "git add"
      ],
      "package.json": [
        "fixpack",
        "git add"
      ]
    }
  },
  "main": "./dist/ziti-browzer-runtime.js",
  "browser": {
    "./dist/ziti-browzer-runtime.js": "./dist/ziti-browzer-runtime.js",
    "./src/node/ziti-browzer-runtime.js": "./src/ziti-browzer-runtime.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-regenerator": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "browserify": "^17.0.0",
    "cross-env": "^7.0.3",
    "mocha": "^9.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "tinyify": "^3.0.0"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.isundefined": "^3.0.1"
  }
}
